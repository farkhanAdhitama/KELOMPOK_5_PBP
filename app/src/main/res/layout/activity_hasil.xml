<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HasilActivity">

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/hasil"
                android:layout_marginTop="30dp"
                android:textSize="25sp"
                android:textColor="#000000"
                android:layout_centerHorizontal="true"
                android:id="@+id/hasiljudul"/>
        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="20sp"
                android:textColor="#000000"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/hasiljudul"
                android:layout_marginTop="10dp"
                android:id="@+id/hasilnama" />
        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="20sp"
                android:textColor="#000000"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/hasiltempat"
                android:layout_marginTop="10dp"
                android:id="@+id/hasiltanggal" />
        </TableRow>

        <TableRow>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="20sp"
                android:textColor="#000000"
                android:layout_centerHorizontal="true"
                android:layout_below="@id/hasilnama"
                android:layout_marginTop="10dp"
                android:id="@+id/hasiltempat" />
        </TableRow>
    </TableLayout>

</RelativeLayout>